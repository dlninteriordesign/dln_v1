<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {% set keywords = '' %}
        {% set description = '' %}
        <meta name="keywords" content="{% block keywords %}{{ keywords }}{% endblock %}" />
        <meta name="description" content="{% block description %}{{ description }}{% endblock %}" />

        <title>{% block title %}DLN Interior Design, Inc.{% endblock %}</title>

        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i" rel="stylesheet">
            <link href="https://fonts.googleapis.com/css?family=Zilla+Slab:300,300i,400,400i,600,600i,700,700i" rel="stylesheet">
            <link href="../css/foundation.min.css" rel="stylesheet" type="text/css" />
            <link href="../css/styles.css" rel="stylesheet" type="text/css" />
        {% endblock %}
    </head>

    <body role="document">
        {% include 'layout/header.html.twig' %}

        <main id="main-container">
            {% block content '' %}
        </main>

        {% include 'layout/footer.html.twig' %}

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
            <script src="../js/foundation.min.js"></script>
            <script src="https://use.fontawesome.com/03da2d3d40.js"></script>
            <script type="text/javascript" src="../js/scripts.js"></script>
        {% endblock %}
    </body>
</html>
